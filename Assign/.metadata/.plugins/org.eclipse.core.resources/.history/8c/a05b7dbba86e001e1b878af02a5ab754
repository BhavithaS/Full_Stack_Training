package example;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;
import java.util.Map;

public class Student {
	private Map<String, List<Integer>> studentData;

    public Student() {
        studentData = new HashMap<>();
    }

    public void addStudent(String name, int score) {
        if (studentData.containsKey(name)) {
            studentData.get(name).add(score);
        } else {
            List<Integer> scores = new ArrayList<>();
            scores.add(score);
            studentData.put(name, scores);
        }
    }

    public List<Integer> getScores(String name) {
        return studentData.get(name);
    }

    public static void main(String[] args) {
    	Student database = new Student();

        // Add scores for students
        database.addStudent("John", 85);
        database.addStudent("Alice", 92);
        database.addStudent("John", 78); // Another John with a different score
        database.addStudent("Bob", 89);
        database.addStudent("Alice", 95); // Another Alice with a different score

        // Get scores for a specific student
        String studentName = "John";
        List<Integer> scores = database.getScores(studentName);

        if (scores != null) {
            System.out.println("Scores for " + studentName + ": " + scores);
        } else {
            System.out.println("Student not found.");
        }
        System.out.println(database.toString());
    }
		
}


    


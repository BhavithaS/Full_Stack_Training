<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="com.spring.app.Loan" id="loan">
        <property name="loanType" value="HomeLoan"/>
        <property name="loanAmount" value="150000.0"/>
    </bean>
    <bean class="com.spring.app.Account" id="accountObj">
        <property name="accNumber" value="335647852"/>
        <property name="accHolderName" value="Vanitha"/>
        <property name="accBalance" value="250000.0"/>
<!--        <constructor-arg name="loanInfo" ref="loan"/>-->
</bean>
</beans>